Exercises/
2. head -15 HM067743.1_cds_ADQ37313.1_1.fa > output.fa
3. ls *.fa | wc -l
4. grep "^>" all.fa
5. grep "^>" all.fa | wc -l
6. cp ../outbreak.csv .; cut -d"," -f3 outbreak.csv | grep -i -v
"multi-country" | sort | uniq
7. cut -d" " -f2 outbreak.csv | grep -i -v "multi-country" | sort |
uniq -c
